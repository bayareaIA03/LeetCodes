public class Main
{
	public static void main(String[] args) {
		int a = -5;
		int b =-1;
		int c = -3;
		int sum = 0;
		for(int i = a;i<=b;i++){
		    sum+=i;
		    //System.out.println(sum);
		    System.out.println(i+" i");
		}
		int z = sum;
		for(int i = b-1;i>=c;i--){
		    sum+=i;
		    System.out.println("2");
		    //System.out.println(sum);
		    System.out.println(i+" i");
		}
		System.out.println(sum);
	}
}
