import java.util.*;
public class Main
{
	public static void main(String[] args) {
		int n = 7;
		int ans = 0;
		boolean isPrime=true;
		
		if(n==0||n==1){  
             System.out.println(n+" is not prime number");      
		}
		
        for(int i=2;i<=n;i++) // loop from 2 to square root of n
        {
        
        if(n%i==0) //Not a prime. if has no remainder(0), not a prime               
            {
            ans=i; // take smallest divisor
            isPrime = false;
            break;
            }
            
        }
        if(!isPrime)//if value of fact has a remainder then it is not a prime number
        System.out.println(ans+" Not prime");
        else
		    System.out.println(1+" is prime");
	}
}
